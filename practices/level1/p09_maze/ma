#include <algorithm>
#include <iostream>
#include <cstdio>
#include <cstring>
#include <vector>
#include <cmath>
#include <ctime>
int map[1100][1100],use[1100][1100],i,j,x,y,n,m;
char s[1100];
int xx[4]={-1,0,1,0};
int yy[4]={0,1,0,-1};
bool search(int x,int y)
{
 int newx,newy,i;
 if (use[x][y]==1) return false;
 use[x][y]=1;
 for (i=0;i<=3;i++)
 {
    newx=x+xx[i];
    newy=y+yy[i];
    if (map[newx][newy]==0) continue;
    if (map[newx][newy]==2) return true;
    if (search(newx,newy)) return true;
 }
 return false;
}
int main()
{
 //假设迷宫是个n*m的矩阵，0代表墙，1代表空地，2代表出口。 
 scanf("%d%d%d%d\n",&n,&m,&x,&y);
 //x,y分表代表起始点坐标 
 for (i=1;i<=n;i++)
 {
   gets(s);
   for (j=1;j<=m;j++) map[i][j]=int(s[j-1])-48;
 }
 if (search(x,y)) printf("YES\n"); else printf("NO\n");
}  
