#include <iostream>
#include <cstdio>
int i,j,use[110];
bool no(int );
void makeprime();
int main()
{
    makeprime();
    for (i=4;i<=100;i+=2)
    if (no(i))
    {
        printf("%d\n",i);
        printf("NO\n");
        return 0;
    }
    printf("YES\n");
}
void makeprime()
{
    int i,j;
    use[1]=1;
    for (i=2;i<=50;i++)
    if (use[i]==0)
    {
        for (j=i+i;j<=100;j+=i) use[j]=1;
    }
}
bool no(int a)
{
    int i;
    for (i=2;i<a;i++)  if (use[i]==0 && use[a-i]==0) return false;
    return true;
}
