#include <algorithm>
#include <iostream>
#include <cstdio>
#include <cstring>
#include <vector>
#include <cmath>
int i,j,use[110];
bool no(int a)
{
 int i;
 for (i=2;i<a;i++)  if (use[i]==0 && use[a-i]==0) return false;
 return true;
}
int main()
{
  use[1]=1;
  for (i=2;i<=50;i++)
  if (use[i]==0)
  {
    for (j=i+i;j<=100;j+=i) use[j]=1;
  }
  for (i=4;i<=100;i+=2)
  if (no(i))
  {
    printf("%d\n",i);
    printf("NO\n");
    return 0;
  }
  printf("YES\n");
}  
