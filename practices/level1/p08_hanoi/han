#include <algorithm>
#include <iostream>
#include <cstdio>
#include <cstring>
#include <vector>
#include <cmath>
#include <ctime>
int a;
void search(char a,char b,char c,int p)
{
 if (p==1)
 {
   printf("%c->%c\n",a,c);
   return;
 }
 search(a,c,b,p-1);
 printf("%c->%c\n",a,c);
 search(b,a,c,p-1);
}
int main()
{
  scanf("%d",&a);
  search('A','B','C',a);
}  
