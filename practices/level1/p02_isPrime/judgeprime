#include <algorithm>
#include <iostream>
#include <cstdio>
#include <cstring>
#include <vector>
#include <cmath>
using namespace std;
int n;
bool can(int a)
{
 int i,r=int(sqrt(a));
 if (a==1) return false;
 for (i=2;i<=r;i++) if (a%i==0) return false;
 return true;
}
int main()
{
  scanf("%d",&n);
  if (can(n)) printf("YES\n"); else printf("NO\n");
}
